{
  "name": "electron-sample",
  "version": "0.0.1",
  "description": "Electron sample app",
  "main": "index.js",
  "scripts": {
    "start": "electron . --debug=5858",
    "watch": "webpack-dev-server",
    "electron-rebuild": "electron-rebuild",
    "lint": "standard",
    "package": "cp  res/app.icns Brave-*/Brave.app/Contents/Resources/atom.icns && asar pack Brave-*/Brave.app/Contents/Resources/app app.asar && rm -Rf Brave-*/Brave.app/Contents/Resources/app && mv app.asar Brave-*/Brave.app/Contents/Resources/",
    "build-darwin": "electron-packager . Brave --overwrite --ignore='electron-packager electron-prebuilt electron-rebuild' --platform=darwin --arch=x64 --version=0.35.1",
    "test": "standard",
    "test-start-chromedriver": "chromedriver",
    "test-integration": "node tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbondy/electron-sample.git"
  },
  "author": "Brian R. Bondy <netzen@gmail.com> (http://www.brianbondy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbondy/electron-sample/issues"
  },
  "homepage": "https://github.com/bbondy/electron-sample#readme",
  "dependencies": {
    "electron-localshortcut": "^0.6.0",
    "electron-prebuilt": "^0.35.1",
    "immutable": "^3.7.5",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "urlutil.js": "^0.1.1"
  },
  "devDependencies": {
    "asar": "^0.8.3",
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chromedriver": "^2.20.0",
    "css-loader": "^0.23.0",
    "electron-packager": "^5.1.1",
    "electron-rebuild": "^1.0.2",
    "gulp": "^3.9.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "node-libs-browser": "^0.5.3",
    "selenium-webdriver": "^2.48.2",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "webdriverio": "^3.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "standard": {
    "ignore": [
      "node_modules",
      "Brave-*"
    ]
  }
}
